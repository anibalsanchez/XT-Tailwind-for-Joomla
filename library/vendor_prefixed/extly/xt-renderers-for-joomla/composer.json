{
  "name": "extly/xt-renderers-for-joomla",
  "description": "XT Tailwind for Extly template, based on Tailwind CSS.",
  "authors": [
    {
      "name": "Extly CB",
      "email": "team@extly.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "anibalsanchez/extly-html-asset-tags-builder": "^1.7.0",
    "doctrine/inflector": "~1.1.0 || ^1.4 || ^2.0",
    "illuminate/support": "~5.4.0 || ^6.0 || ^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Extly\\CMS\\Document\\Renderer\\Html\\": "src/"
    },
    "files": [
      "src/ClassAliasMap.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.33"
    },
    "platform-check": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "php-prefixer": {
      "case-1": "includeLibrariesDependiencies: Process additional dependencies",
      "case-2": "ignorePaths: Do not prefix.",
      "case-3": "ignoreLibraries: Do not prefix.",
      "case-4": "Do not prefix.",
      "case-5": "excludeFromClassmap: Libraries to be removed, but keep dependecies consistent (Platform Lib).",
      "case-6": "excludePaths: Files to be removed from the prefixer output.",
      "case-7": "Process additional project directories",
      "ignoreGlobalObjects": [],
      "ignoreLibraries": [],
      "includeExtraSourcePaths": [
        "template"
      ],
      "prefixForGlobalObjects": "XTP_",
      "prefixForNamespaces": "XTP_BUILD",
      "projectName": "XT Tailwind for Extly"
    }
  }
}
