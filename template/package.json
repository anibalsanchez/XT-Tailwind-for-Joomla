{
  "name": "xt-tailwind-for-joomla",
  "description": "XT Tailwind - My Blog template, based on Tailwind CSS.",
  "scripts": {
    "dev-cli": "npx tailwindcss -i ./src/styles.css -o ./dist/main.css --watch",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "serve": "cross-env NODE_ENV=development webpack-dev-server",
    "dev-proxy": "cross-env NODE_ENV=development webpack --watch",
    "dev-build": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "prod": "cross-env NODE_ENV=production webpack",
    "update-browserslist": "npx browserslist@latest --update-db"
  },
  "keywords": [
    "Tailwind, CSS, Webpack, PostCSS, cssnano"
  ],
  "author": "Extly CB",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.6",
    "postcss-loader": "^6.2.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.15",
    "cssnano-preset-advanced": "^5.1.10",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.13"
  },
  "config-disabled": {
    "proxyURL": "http://localhost:8080/index.php",
    "extraCCProxyFolder": "/var/www/html/templates/xttailwindcss"
  }
}
